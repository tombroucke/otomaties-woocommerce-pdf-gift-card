{
    "name": "tombroucke/otomaties-woocommerce-pdf-gift-card",
    "description": "Sell PDF gift cards through WooCommerce with a custom design.",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Tom Broucke",
            "email": "tom@tombroucke.be"
        }
    ],
    "require": {
        "php": "^8.0",
        "composer/installers": "^2.0",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.1",
        "illuminate/support": "^9.33"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "require-dev": {
        "szepeviktor/phpstan-wordpress": "^1.1",
        "php-stubs/woocommerce-stubs": "^6.9"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpstan analyze includes otomaties-woocommerce-pdf-gift-card.php --memory-limit 1024M"
        ]
    }
}
